#set ($staticFieldVisibility = $staticField.Descriptor.Visibility)
#set ($staticFieldType = ${TranspilerUtils.getJsDocName($staticField.Descriptor.Type)})
#set ($staticFieldName = ${ManglingNameUtils.getMangledName($staticField.Descriptor)})
#set ($directStaticFieldAccess = "$" + ${staticFieldName})


/**
 * @$staticFieldVisibility {?$staticFieldType}
 */
$className.$directStaticFieldAccess = ${TranspilerUtils.getDefaultInitialValue($staticField.Descriptor.Type)};


window.Object.defineProperties($className, {
  $staticFieldName: {
    get: function() {
      $className.$clinit();
      return $className.$directStaticFieldAccess;
    },
    set: function(value) {
      $className.$clinit();
      $className.$directStaticFieldAccess = value;
    }
  }
});
